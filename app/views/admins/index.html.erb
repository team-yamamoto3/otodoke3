<div class="container">
	<h1>ユーザー検索画面</h1>
		<div class="row">
			<p>ユーザー検索</p>
			<%= form_tag admins_index_path, :method => 'get' do %>
				<p>姓</p>
  					<%= text_field_tag :search, params[:search] %>
			<p>名</p>
  				<%= text_field_tag :kennsaku, params[:kennsaku] %>
  				<%= submit_tag "ユーザーを検索する", :name => nil %>
			<% end %>
		</div>
		<div class="row">
			<table class="table table-striped">
				<% if @endusers %>
				<% @endusers.each do |enduser| %>
				<tr>
					<td><%= enduser.last_name %><%= enduser.first_name %></td>
					<td><%= link_to "ユーザー詳細", enduser_path(enduser.id) %></td>
				</tr>
				<% end %>
				<% end %>
			</table>
		</div>
</div>