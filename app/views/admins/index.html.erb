<div class="container">
	<h1>ユーザー一覧</h1>
		<div class="row">
			<%= search_form_for @q, url: admins_index_path do |f| %>
  			<%= f.label :last_name_cont_and_first_name_cont, "名前" %>
  			<%= f.search_field :last_name_cont_and_first_name_cont %>
  			<%= f.submit "検索" %>
			<% end %>
		</div>
		<div class="row">
			<% @endusers.each do |enduser| %>
			<tr>
				<td><%= enduser.last_name %><%= enduser.first_name %></td>
			</tr>
			<% end %>
		</div>
</div>